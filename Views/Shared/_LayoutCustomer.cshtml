                            <li><a href="@Url.Action("Contact","Home")">Contact Us</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Main Top -->
    <!-- Start Main Top -->
    <header class="main-header">
        <!-- Start Navigation -->
        <nav class="navbar navbar-expand-lg navbar-light bg-light navbar-default bootsnav">
            <div class="container">
                <!-- Start Header Navigation -->
                <div class="navbar-header">
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-menu" aria-controls="navbars-rs-food" aria-expanded="false" aria-label="Toggle navigation">
                        <i class="fa fa-bars"></i>
                    </button>
                    <a class="navbar-brand" href="@Url.Action("IndexCustomer","Home")"><img  src="@Url.Content(db.Companies.Select(x=>x.COMPANY_LOGO).FirstOrDefault())" class="logo" height="82px" width="200px" alt=""></a>
                </div>
                <!-- End Header Navigation -->
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="navbar-menu">
                    <ul class="nav navbar-nav ml-auto" data-in="fadeInDown" data-out="fadeOutUp">
                        <li class="nav-item active"><a class="nav-link" href="@Url.Action("IndexCustomer","Home")">Home</a></li>
                        <li class="nav-item"><a class="nav-link" href="@Url.Action("About","Home")">About Us</a></li>
                        <li class="nav-item"><a class="nav-link" href="@Url.Action("Shop","Home")">Product</a></li>

                        <li class="nav-item"><a class="nav-link" href="@Url.Action("Contact","Home")">Contact Us</a></li>
                        <li class="nav-item"><a class="nav-link" href="@Url.Action("MyAccount","Home")">My Account</a></li>
                        <li class="dropdown">
                            @if (Session["Customer"] == null)
                            {
                                <a href="#" class="nav-link dropdown-toggle arrow" data-toggle="dropdown">Sign up </a>
                                <ul class="dropdown-menu">

                                    <li><a href="@Url.Action("Create","Customers")">Create Account</a></li>
                                    <li><a href="@Url.Action("Login","Customers")">Login <i class="fa fa-sign-in"></i></a></li>
                                </ul>
                            }
                            else
                            {
                            <li class="nav-item"><a class="nav-link" href="@Url.Action("Logout","Customers")">Logout  <i class="fa fa-sign-out"></i></a></li>
                            }


                            </li>
                        </ul>
                </div>
                <!-- /.navbar-collapse -->
                <!-- Start Atribute Navigation -->
                <div class="attr-nav">
                    <ul>
                        <li class="search"><a href="@Url.Action("Login","Customers")"><i class="fa fa-user"></i></a></li>
                        <li class="side-menu">
                            <a href="@Url.Action("Cart","Home")">
                                <i class="fa fa-shopping-bag"></i>
                                <span class="badge">
                                    @if(Session["myCart"]!=null)
                                    {
                                        List<Product> list= (List < Product >) Session["myCart"];
                                        @list.Count();
                                    }
                                </span>
                            </a>
                        </li>
                        
                    </ul>
                </div>
                <!-- End Atribute Navigation --
            </div>
            <!-- Start Side Menu -->
            <div class="side">
                <a href="#" class="close-side"><i class="fa fa-times"></i></a>
                <li class="cart-box">
                    <ul class="cart-list">
                        <li>
                            <a href="#" class="photo"><img src="~/Content/Customer/images/img-pro-01.jpg" class="cart-thumb" alt="" /></a>
                            <h6><a href="#">Delica omtantur </a></h6>
                            <p>1x - <span class="price">$80.00</span></p>
                        </li>
                        <li>
                            <a href="#" class="photo"><img src="~/Content/Customer/images/img-pro-02.jpg" class="cart-thumb" alt="" /></a>
                            <h6><a href="#">Omnes ocurreret</a></h6>
                            <p>1x - <span class="price">$60.00</span></p>
                        </li>
                        <li>
                            <a href="#" class="photo"><img src="~/Content/Customer/images/img-pro-03.jpg" class="cart-thumb" alt="" /></a>
                            <h6><a href="#">Agam facilisis</a></h6>
                            <p>1x - <span class="price">$40.00</span></p>
                        </li>
                        <li class="total">
                            <a href="#" class="btn btn-default hvr-hover btn-cart">VIEW CART</a>
                            <span class="float-right"><strong>Total</strong>: $180.00</span>
                        </li>
                    </ul>
                </li>
            </div>
            <!-- End Side Menu -->
        </nav>
        <!-- End Navigation -->
    </header>
